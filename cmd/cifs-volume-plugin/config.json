{
  "description": "CIFS volume provider",
  "documentation": "https://raw.githubusercontent.com/wwmoraes/docker-engine-plugins/master/cifs-volume-plugin/README.md",
  "entrypoint": [
    "tini",
    "--",
    "cifs-volume-plugin"
  ],
  "env": [
    {
      "description": "1Password Connect host URL",
      "name": "OP_CONNECT_HOST",
      "settable": [
        "value"
      ],
      "value": "http://localhost:8080"
    },
    {
      "description": "1Password Connect access token",
      "name": "OP_CONNECT_TOKEN_FILE",
      "settable": [
        "value"
      ],
      "value": "/run/secrets/op/token"
    }
  ],
  "interface": {
    "socket": "cifs.sock",
    "types": [
      "docker.volumedriver/1.0"
    ]
  }
}
